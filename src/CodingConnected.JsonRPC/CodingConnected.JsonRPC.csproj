<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <LangVersion>latest</LangVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Title>CodingConnected.JsonRPC</Title>
    <Authors>CodingConnected e.U., Menno van der Woude</Authors>
    <Description>A compact library to ease using json-rpc in .NET</Description>
    <Copyright>Copyright (C) 2018-2020 CodingConnected e.U.</Copyright>
    <PackageTags>json, rpc</PackageTags>
    <PackageProjectUrl>https://github.com/CodingConnected/CodingConnected.JsonRPC</PackageProjectUrl>
    <PackageLicense>MIT</PackageLicense>
    <RepositoryUrl>https://github.com/CodingConnected/CodingConnected.JsonRPC</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <TargetFramework>netstandard2.0</TargetFramework>
  </PropertyGroup>
  <Import Project="..\..\.paket\Paket.Restore.targets" />
</Project>